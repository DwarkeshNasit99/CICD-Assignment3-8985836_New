{
  "name": "azure-function-cicd-assignment",
  "version": "1.0.0",
  "description": "CI/CD Pipeline for Azure Functions using Jenkins",
  "main": "src/index.js",
  "scripts": {
    "start": "func start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm install",
    "prestart": "npm run extensions:install",
    "extensions:install": "func extensions install"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.5",
    "axios": "^1.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Student Assignment",
  "license": "MIT"
}